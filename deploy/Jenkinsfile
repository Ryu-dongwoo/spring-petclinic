      }
    }
    stage('Maven Build') {
      steps {
        echo 'Maven build'
      }
    }
    stage('Docker Image') {
      steps {
        echo 'Docker Image'
      }
    }
    stage('Image Push') {
      steps {
        echo 'Image Push'
      }
    }
    stage('Codedeploy Workload') {
      steps {
        echo 'Codedeploy Workload'
      }
    }
  }
}
